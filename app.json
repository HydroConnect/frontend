{
    "expo": {
        "name": "HydroConnect",
        "slug": "fe",
        "version": "1.0.0",
        "runtimeVersion": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon-hc.png",
        "scheme": "hydroconnect",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "bundleIdentifier": "com.hydroconnect.app",
            "supportsTablet": true
        },
        "android": {
            "package": "com.hydroconnect.app",
            "adaptiveIcon": {
                "backgroundColor": "#E6F4FE",
                "foregroundImage": "./assets/images/icon-hc.png",
                "backgroundImage": "./assets/images/icon-hc.png",
                "monochromeImage": "./assets/images/icon-hc.png"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "googleServicesFile": "./google-services.json"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/icon-hc.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-notifications",
                {
                    "icon": "./assets/images/icon-hc.png"
                }
            ],
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/icon-hc.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#000000",
                    "dark": {
                        "backgroundColor": "#000000"
                    }
                }
            ],
            "expo-font",
            "expo-web-browser",
            "expo-audio",
            "@react-native-firebase/app"
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "983010ea-c49f-461f-be4d-c0d1fa8a684e"
            }
        }
    }
}
